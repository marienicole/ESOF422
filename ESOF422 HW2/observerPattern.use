-- This is the USE model for a generic Observer pattern.

model ObserverPattern

class Subject
attributes

operations
	attach(o:Observer)
		begin
		end
	detach(o:Observer)
	notify()
end

class Observer	
attributes
operations
	update()
end

class ConcreteSubject
attributes
	subjectState:State
operations
	getState():State
	setState(s:State)
end

class ConcreteObserver
attributes
	observerState:State
operations
	getState():State
end

